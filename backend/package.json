{
  "name": "template-react",
  "version": "1.0.0",
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "main": "src/server.ts",
  "scripts": {
    "test": "jest --watchAll --no-cache",
    "test:ci": "jest --no-cache",
    "server": "ts-node-dev --transpile-only --respawn ./src/server.js",
    "client": "npm start --prefix client",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Armand72/template-react.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Armand72/template-react/issues"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "<rootDir>/client"
    ],
    "setupFilesAfterEnv": [
      "./src/routes/__test__/setup.ts"
    ],
    "globals": {
      "jest": {
        "isolatedModules": true
      }
    }
  },
  "homepage": "https://github.com/Armand72/template-react#readme",
  "dependencies": {
    "@typegoose/typegoose": "^7.6.1",
    "@types/mongoose": "^5.11.97",
    "apollo-server": "^2.25.0",
    "apollo-server-errors": "^2.5.0",
    "apollo-server-express": "^2.25.2",
    "bcrypt": "^5.0.1",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "graphql-upload": "^12.0.0",
    "jest": "26.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.9",
    "mongoose": "5.10.18",
    "react": "^17.0.2",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "typescript": "^4.3.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/graphql-upload": "^8.0.4",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/mongodb": "^3.6.18",
    "@types/node": "^15.12.1",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.28.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.1",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6"
  }
}
